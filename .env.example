# .env.example - template for required environment variables

# Port for the server to listen on (default: 3000 for local development)
# AWS environments (like Elastic Beanstalk, ECS, Lambda, etc.) may override this value automatically.
# If deploying on AWS, check the platform's documentation for the correct port value.
PORT=3000

# Your Supabase project URL
SUPABASE_URL=your-supabase-url-here

# Your Supabase service role key (keep this secret in production)
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here
